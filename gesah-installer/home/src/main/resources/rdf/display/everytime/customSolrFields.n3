    rdfs:label "material" ;
    :hasTargetField "material_ss" ;
    :hasSelectQuery """
PREFIX rdfs:     <http://www.w3.org/2000/01/rdf-schema#>
PREFIX gesah:    <http://ontology.tib.eu/gesah/>
SELECT ?material 
WHERE
{
