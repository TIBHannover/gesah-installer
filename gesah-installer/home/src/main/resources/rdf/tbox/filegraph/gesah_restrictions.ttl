gesah:comment rdfs:domain [ a owl:Class; owl:unionOf (gesah:Watermark gesah:Collection gesah:Feature gesah:Inscription gesah:Mark gesah:Series gesah:Watermark ecrm:E35_Title obo:BFO_0000015 gesah:Object_Category_Generic_Term gesah:Object_Category_Narrower_Term )] .	   
gesah:uses_technique rdfs:domain [ a owl:Class; owl:unionOf (gesah:Creation gesah:Edition gesah:Production )] .	   
