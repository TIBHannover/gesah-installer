ecrm:E35_Title rdfs:subClassOf 
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty gesah:CONA_ID ;
                                 owl:allValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty gesah:GND_ID ;
                                 owl:allValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty gesah:comment ;
                                 owl:allValuesFrom rdf:XMLLiteral
                               ] .
							   
							   
